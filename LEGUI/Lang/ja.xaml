<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <FontFamily x:Key="UIFont">Yu Gothic UI</FontFamily>
    <system:String x:Key="EnterArgument">ここに引数を入力してください (ある場合)</system:String>
    <system:String x:Key="Save">保存</system:String>
    <system:String x:Key="Shortcut">ショートカットを作成</system:String>
    <system:String x:Key="Cancel">キャンセル</system:String>
    <system:String x:Key="SaveAs">名前を付けて保存...</system:String>
    <system:String x:Key="SaveAsInstruction">新しいプロファイル名:</system:String>
    <system:String x:Key="Delete">削除</system:String>
    <system:String x:Key="ConfirmDel">このプロファイルを削除してもよろしいですか？</system:String>
    <system:String x:Key="Location">ロケーション</system:String>
    <system:String x:Key="LocationSettings">ロケーションの設定</system:String>
    <system:String x:Key="TimezoneSettings">タイムゾーンの設定</system:String>
    <system:String x:Key="Timezone">タイムゾーン</system:String>
    <system:String x:Key="DebugOptions">高度なオプション</system:String>
    <system:String x:Key="AsAdmin">管理者として実行</system:String>
    <system:String x:Key="RedirectRegistry">レジストリ内の言語関連キーを偽装</system:String>
    <system:String x:Key="IsAdvancedRedirection">システム UI 言語を偽装</system:String>
    <system:String x:Key="WithCREATESUSPENDED">CREATE__SUSPENDED でプロセスを作成</system:String>
    <system:String x:Key="Miscellaneous">その他</system:String>
    <system:String x:Key="ShowInMainMenu">このプロファイルをメイン メニューに表示</system:String>
</ResourceDictionary>
